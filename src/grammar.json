{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "amml",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "expression"
          },
          {
            "type": "SYMBOL",
            "name": "note"
          }
        ]
      }
    },
    "note": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\nNOTE("
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "\\w+"
          },
          "named": true,
          "value": "note_format"
        },
        {
          "type": "STRING",
          "value": ")\n"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "REPEAT1",
            "content": {
              "type": "PATTERN",
              "value": "."
            }
          },
          "named": true,
          "value": "note_text"
        },
        {
          "type": "STRING",
          "value": "\nENDNOTE\n"
        }
      ]
    },
    "expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "create_var"
            },
            {
              "type": "SYMBOL",
              "name": "number"
            },
            {
              "type": "SYMBOL",
              "name": "operator"
            },
            {
              "type": "SYMBOL",
              "name": "variable"
            },
            {
              "type": "SYMBOL",
              "name": "func_call"
            },
            {
              "type": "SYMBOL",
              "name": "for_range"
            },
            {
              "type": "SYMBOL",
              "name": "subtraction_bar"
            },
            {
              "type": "SYMBOL",
              "name": "set"
            },
            {
              "type": "SYMBOL",
              "name": "string"
            }
          ]
        }
      ]
    },
    "string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "PATTERN",
          "value": "[^\"\\n]+"
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "func_call": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "funcname",
          "content": {
            "type": "SYMBOL",
            "name": "variable"
          }
        },
        {
          "type": "SYMBOL",
          "name": "func_params"
        }
      ]
    },
    "set": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "SYMBOL",
                        "name": "expression"
                      },
                      {
                        "type": "STRING",
                        "value": ","
                      }
                    ]
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              },
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "expression"
                  },
                  {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "BLANK"
                      }
                    ]
                  }
                ]
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "variable": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "[^\\d\\s(){}]"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "PATTERN",
              "value": "\\w"
            }
          }
        ]
      }
    },
    "func_params": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "SYMBOL",
                        "name": "expression"
                      },
                      {
                        "type": "STRING",
                        "value": ","
                      }
                    ]
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              },
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "expression"
                  },
                  {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "BLANK"
                      }
                    ]
                  }
                ]
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "subtraction_bar": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "STRING",
            "value": "|"
          },
          "named": true,
          "value": "operator"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "ALIAS",
              "content": {
                "type": "STRING",
                "value": "^"
              },
              "named": true,
              "value": "operator"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "STRING",
            "value": ".."
          },
          "named": true,
          "value": "operator"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "ALIAS",
              "content": {
                "type": "STRING",
                "value": "_"
              },
              "named": true,
              "value": "operator"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "number"
        }
      ]
    },
    "create_var": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "STRING",
            "value": "let"
          },
          "named": true,
          "value": "keyword"
        },
        {
          "type": "SYMBOL",
          "name": "variable"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "func_params"
            },
            {
              "type": "BLANK"
