            "type": "STRING",
            "value": "="
          },
          "named": true,
          "value": "operator"
        },
        {
          "type": "SYMBOL",
          "name": "expression"
        }
      ]
    },
    "number": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\d+"
        },
        {
          "type": "IMMEDIATE_TOKEN",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": "."
                  },
                  {
                    "type": "PATTERN",
                    "value": "\\d+"
                  }
                ]
              },
              {
                "type": "BLANK"
              }
            ]
          }
        }
      ]
    },
    "for_range": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "STRING",
            "value": "for"
          },
          "named": true,
          "value": "keyword"
        },
        {
          "type": "SYMBOL",
          "name": "variable"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "STRING",
            "value": "="
